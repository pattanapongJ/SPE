<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="hr_expence_sequence_number_custom" model="ir.sequence">
            <field name="name">HR Expense Sequence Number</field>
            <field name="code">hr.expense.sequence.custom</field>
            <field name="prefix">EXPENSE/%(month)s/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="hr_expence_report_sequence_number_custom" model="ir.sequence">
            <field name="name">HR Expense report Sequence Number</field>
            <field name="code">hr.expense.report.sequence.custom</field>
            <field name="prefix">EXPENSE/REPORT/%(month)s/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>

