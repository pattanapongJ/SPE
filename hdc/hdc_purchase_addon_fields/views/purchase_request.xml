<odoo>
    <record id="purchase_request_form_team" model="ir.ui.view">
        <field name="name">purchase.request.form.team</field>
        <field name="model">purchase.request</field>
        <field name="inherit_id" ref="purchase_request.view_purchase_request_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='line_ids']/tree" position="inside">
                <field name="team_id" optional="show" readonly="1" force_save="1"/>
            </xpath>

            <xpath expr="//field[@name='assigned_to']" position="after">
                <field name="pr_reference" />
            </xpath>
        </field>
    </record>

    <record id="inherit_purchase_request_report" model="ir.ui.view">
        <field name="name">inherit.purchase.request.report</field>
        <field name="model">purchase.request</field>
        <field name="inherit_id" ref="purchase_request_report.purchase_request_form_view_inherit"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='for_customer']" position="after">
                <field name="free_text_customer" />
            </xpath>
        </field>
    </record>
</odoo>