<?xml version="1.0"?>
<odoo>
    <record id="hdc_tms.resupply_delivery_round_master_view_action" model="ir.actions.act_window">
        <field name="name">Delivery Line</field>
        <field name="res_model">delivery.round</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
          <p class="o_view_nocontent_smiling_face">
            Define a new Delivery Line
          </p>
        </field>
    </record>

    <record id="delivery_round_view_form_inherit" model="ir.ui.view">
      <field name="name">delivery.round.view.form.inherit</field>
      <field name="model">delivery.round</field>
      <field name="inherit_id" ref="hdc_tms.resupply_delivery_round_master_view_form"/>
      <field name="arch" type="xml">
          <xpath expr="//field[@name='name']" position="after">
              <field name="code"/>
          </xpath>
      </field>
    </record>

    
</odoo>
