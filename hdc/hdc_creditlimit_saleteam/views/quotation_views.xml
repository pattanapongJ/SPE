<odoo>
    <record id="quotation_order_inherit_credit_limit" model="ir.ui.view">
        <field name="name">quotation.order.form.inherit.retail</field>
        <field name="model">quotation.order</field>
        <field name="inherit_id" ref="hdc_quotation_order.view_quotation_order_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='type_id']" position="after">
                <field name="is_credit_limit" readonly="1" force_save="1" />
            </xpath>
        </field>
    </record>
</odoo>