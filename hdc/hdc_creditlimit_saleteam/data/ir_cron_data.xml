<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="ir_cron_check_temp_credit_request_approval_expiry" model="ir.cron">
            <field name="name">Credit Limit : Check Temp Credit Request Approval Expiry</field>
            <field name="model_id" ref="model_temp_credit_request"/>
            <field name="state">code</field>
            <field name="code">model.check_approval_expiry(days=7)</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">1</field>
            <field name="doall" eval="False"/>
            <field name="priority">5</field>
            <field name="active" eval="False"/>
        </record>
        <record id="ir_cron_create_partner_cash_limit" model="ir.cron">
            <field name="name">Credit Limit : Create Partner Cash Limit</field>
            <field name="model_id" ref="model_res_partner"/>
            <field name="state">code</field>
            <field name="code">model.create_partner_cash_limit()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">1</field>
            <field name="doall" eval="False"/>
            <field name="priority">5</field>
            <field name="active" eval="False"/>
        </record>
    </data>
</odoo>
