<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_sale_inter_warning_form" model="ir.ui.view">
        <field name="name">sale.inter.company.warning.wizard.form</field>
        <field name="model">sale.inter.company.warning.wizard</field>
        <field name="arch" type="xml">
            <form string="Warning">
                <group>
