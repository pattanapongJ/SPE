<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="stock_location_mkey_cus" model="stock.location">
            <field name="name">MKEY-CUS</field>
            <field name="location_id" model="stock.location"
                eval="obj().env.ref('stock.warehouse0').view_location_id.id"/>
            <field name="usage">internal</field>
            <field name="company_id" ref="base.main_company"/>
        </record>

        <record id="stock_picking_type_receipt_mtk" model="stock.picking.type">
            <field name="name">Receipt MTK</field>
            <field name="code">incoming</field>
            <field name="sequence_code">MKEY</field>
            <field name="company_id" ref="base.main_company"/>
            <field name="warehouse_id" ref="stock.warehouse0"/>
            <field name="default_location_src_id" ref="stock.stock_location_customers"></field>
            <field name="default_location_dest_id" ref="stock_location_mkey_cus"></field>
            <field name="is_master_key">True</field>
        </record>

        <record id="stock_picking_type_transfer_mtk" model="stock.picking.type">
            <field name="name">Transfer MTK</field>
            <field name="code">outgoing</field>
            <field name="sequence_code">TFMKEY</field>
            <field name="company_id" ref="base.main_company"/>
            <field name="warehouse_id" ref="stock.warehouse0"/>
            <field name="default_location_dest_id" ref="stock.stock_location_customers"></field>
            <field name="default_location_src_id" ref="stock_location_mkey_cus"></field>
            <field name="is_transfer_master_key">True</field>
        </record>

        <record id="stock_picking_type_internal_transfer_mtk" model="stock.picking.type">
            <field name="name">Internal Transfer MTK</field>
            <field name="code">internal</field>
            <field name="sequence_code">INTMTK</field>
            <field name="company_id" ref="base.main_company"/>
            <field name="warehouse_id" ref="stock.warehouse0"/>
            <field name="default_location_dest_id" ref="stock_location_mkey_cus"></field>
            <field name="default_location_src_id" ref="__export__.stock_location_2116_1f43bf38"></field>
            <!-- <field name="return_picking_type_id" ref="stock.picking_type_in"></field> -->
            <field name="is_internal_transfer_master_key">True</field>
        </record>
    </data>
</odoo>
