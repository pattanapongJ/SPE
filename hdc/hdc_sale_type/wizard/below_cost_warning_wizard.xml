<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="below_cost_warning_wizard_form" model="ir.ui.view">
        <field name="name">below.cost.warning.wizard.form</field>
        <field name="model">below.cost.warning.wizard</field>
        <field name="arch" type="xml">
            <form string="Below Cost Warning Sale Order">
                <group>
                    <field name="message" readonly="1" />
                </group>
                <footer>
                    <button string="Confirm" type="object" name="action_confirm_wizard"
                        class="btn-primary" />
                    <button string="Cancel" special="cancel" class="btn-secondary" />
                </footer>
            </form>
        </field>
    </record>
</odoo>