<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="stock_picking_batch_form_inherit" model="ir.ui.view">
        <field name="name">stock.picking.batch.form.inherit</field>
        <field name="model">stock.picking.batch</field>
        <field name="inherit_id" ref="stock_picking_batch.stock_picking_batch_form"/>
        <field name="arch" type="xml">
            <xpath expr="//header/button[@name='action_print']" position="replace">
                <button name="do_batch_print_picking" states="in_progress,done" groups="stock.group_stock_user" string="Print Receipt List" type="object"/>
            </xpath>
        </field>
    </record>

</odoo>