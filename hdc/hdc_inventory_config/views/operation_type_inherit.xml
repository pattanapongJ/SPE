<odoo>
    <record id="view_stock_picking_type_inherit" model="ir.ui.view">
        <field name="name">stock.picking.type.form.inherit</field>
        <field name="model">stock.picking.type</field>
        <field name="inherit_id" ref="stock.view_picking_type_form" />
        <field name="arch" type="xml">
            <xpath expr="//form//sheet//group[2]//group[3]" position="before">
                <group string="Addition Operation Types">
                    <field name="addition_operation_types"/>
                    <field name="is_internal_transfer"/>
                </group>
            </xpath>
        </field>
    </record>
</odoo>
