<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="wizard_commission_report_form_view" model="ir.ui.view">
        <field name="name">wizard.commission.report.form.view</field>
        <field name="model">wizard.commission.report</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="settle_commissions_id" invisible="1"/>
                            <field name="target_type" widget="radio" invisible="1"/>
                            <field name="settle_type" widget="radio" invisible="1"/>
                            <field name="commission_report_configuration_id" options="{'no_create': True, 'no_open': True}"/>
                        </group>
                    </group>
                </sheet>
                <footer>
                    <button string="Print" type="object" name="print" class="oe_highlight"/>
                    <button string="Cancel" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

	<record id="action_wizard_commission_report" model="ir.actions.act_window">
		<field name="name">Report</field>
		<field name="type">ir.actions.act_window</field>
		<field name="res_model">wizard.commission.report</field>
		<field name="view_mode">form</field>
		<field name="target">new</field>
		<field name="view_id" ref="wizard_commission_report_form_view"/>
	</record>
</odoo>