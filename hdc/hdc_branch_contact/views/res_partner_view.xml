<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="hdc_head_office_add_field_inherit" model="ir.ui.view">
        <field name="name">hdc.office.field.inherit</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="branch.view_partner_structured_form_branch" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='branch_id']" position="before">
                <field name="is_head_office" />
            </xpath>
        </field>
    </record>
</odoo>

	
