<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_wizard_edit_fields_not_sync" model="ir.ui.view">
        <field name="name">wizard.edit.fields.not.sync</field>
        <field name="model">wizard.edit.fields.not.sync</field>
        <field name="arch" type="xml">
            <form string="Edit Fields Not Sync">
                <group>
                    <group>
                        <field name="obj_id" invisible="1"/>
                    </group>
                    <group>
                    </group>
                </group>

                <notebook>
                    <page name="fields_not_sync" string="Fields Not Sync.">
                        <field name="wizard_edit_fields_not_sync_line" string="Fields Not Sync." nolabel="1">
                            <tree editable="top" create="0" delete="0">
                                <field name="wizard_edit_fields_not_sync_id" invisible="1"/>
                                <field name="selected"/>
                                <field name="name" readonly="1" force_save="1"/>
                                <field name="field_description" readonly="1" force_save="1"/>
                            </tree>
                        </field>
                    </page>
                </notebook>
                <footer>
                    <button name="action_edit" string="Edit" type="object" class="btn-primary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel" />
                </footer>
            </form>
        </field>
    </record>
    
</odoo>
