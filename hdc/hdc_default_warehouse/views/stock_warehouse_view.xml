<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_warehouse_form_inherited_default_warehouse" model="ir.ui.view">
        <field name="name">view.warehouse.form.inherited.default.warehouse</field>
        <field name="model">stock.warehouse</field>
        <field name="inherit_id" ref="stock.view_warehouse"/>
        <field name="priority">20</field>
        <field name="arch" type="xml">
            <xpath expr="//group[1]/group[2]" position="inside">
                <field name="is_default_warehouse"/>
           </xpath>
        </field>
    </record>

</odoo>
