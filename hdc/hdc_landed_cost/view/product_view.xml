<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="hdc_landed_cost_product_tem_view" model="ir.ui.view">
        <field name="name">product.tem.view.hdc_landed_cost</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//group[@name='bill']/field[@name='purchase_method']" position="after">
                <field name="landed_cost_type" />
            </xpath>
                
        </field>
    </record>

</odoo>