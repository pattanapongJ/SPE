<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="product_template_mrp_std_price_form_view" model="ir.ui.view">
        <field name="name">product.template.mrp.std.price.form</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_form_view" />
        <field name="priority" eval="10" />
        <field name="arch" type="xml">
            <xpath expr="//notebook/page[@name='inventory']" position="after">
                <page string="Manufacture">
                    <group name="fac_mrp_group" string="Factory Price">
                        <field name="factory_cost"/>
                        <field name="mrp_margin"/>
                        <field name="factory_sale_price"/>
                    </group>
                    <group name="fac_mrp_group2">
                    </group>
                </page>
            </xpath>
        </field>
    </record>

</odoo>