<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="confirm_below_cost_wizard_form" model="ir.ui.view">
        <field name="name">confirm.below.cost.wizard.form</field>
        <field name="model">confirm.below.cost.wizard</field>
        <field name="arch" type="xml">
            <form string="Below Cost Warning">
                <group>
                    <field name="message" readonly="1"  nolabel="1"/>
                </group>
                <footer>
                    <button string="Confirm" type="object" name="action_confirm_wizard"
                        class="btn-primary" />
                    <button string="Cancel" special="cancel" class="btn-secondary" />
                </footer>
            </form>
        </field>
    </record>
</odoo>