<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="paperformat_report_tms" model="report.paperformat">
        <field name="name">Report TMS A4 Landscape</field>
        <field name="default" eval="True" />
        <field name="format">custom</field>
        <field name="page_height">297</field>
        <field name="page_width">210</field>
        <field name="orientation">Landscape</field>
        <field name="margin_top">55</field>
        <field name="margin_bottom">40</field>
        <field name="margin_left">3</field>
        <field name="margin_right">3</field>
        <field name="header_line" eval="False" />
        <field name="header_spacing">53</field>
        <field name="dpi">90</field>
    </record>

    <record id="paperformat_report_tms_all" model="report.paperformat">
        <field name="name">Report TMS All A4 Portrait</field>
        <field name="default" eval="True" />
