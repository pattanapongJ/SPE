<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="res_partner_bank_type_inherit" model="ir.ui.view">
        <field name="name">res.partner.bank.form.inherit</field>
        <field name="model">res.partner.bank</field>
        <field name="inherit_id" ref="base.view_partner_bank_form"/>
        <field name="arch" type="xml">
           <xpath expr="//group/field[@name='partner_id']" position="after">
               <field name="bank_account_type" string="Bank Account Type"></field>
            </xpath>
            <xpath expr="//group/field[@name='active']" position="after">
               <field name="bank_branch" string="Bank Branch"></field>
            </xpath>
        </field>
    </record>

</odoo>