<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- View cheque.setting form -->
    <record id="account_asset_view_tree_inherit" model="ir.ui.view">
        <field name="name">account_asset_view_tree_inherit</field>
        <field name="model">account.asset</field>
        <field name="inherit_id" ref="account_asset_management.account_asset_view_tree"/>
        <field name="priority">1</field>
        <field name="arch" type="xml">

            <xpath expr="//tree" position="inside">
                <header>
                    <button name="compute_depreciation_board" icon="fa-gears" type="object"
                            class="oe_highlight btn-primary" string="Compute Depreciation"/>
                </header>
            </xpath>


        </field>
    </record>

</odoo>