<odoo>
    <record id="account_asset_holder_view_form" model="ir.ui.view">
        <field name="name">account.asset.holder.form</field>
        <field name="model">account.asset</field>
        <field name="inherit_id" ref="account_asset_management.account_asset_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='analytic_tag_ids']" position="after">
                <field name="asset_holder" placeholder="Select Asset Holder" 
                options="{'no_create': True, 'no_open': True}"/>
            </xpath>
        </field>
    </record>
</odoo>