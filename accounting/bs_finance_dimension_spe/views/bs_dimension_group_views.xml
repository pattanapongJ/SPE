<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <!-- View bs.dimension.group View Tree -->
        <record id="view_bs_dimension_group_tree" model="ir.ui.view">
            <field name="name">view.bs.dimension.group.tree</field>
            <field name="model">bs.dimension.group</field>
            <field name="inherit_id" ref="bs_finance_dimension.view_bs_dimension_group_tree"/>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="ref_model"/>
                </tree>
            </field>
        </record>

        <!-- View bs.dimension.group form -->
        <record id="view_bs_dimension_group_form" model="ir.ui.view">
            <field name="name">view.bs.dimension.group.form</field>
            <field name="model">bs.dimension.group</field>
            <field name="inherit_id" ref="bs_finance_dimension.view_bs_dimension_group_form"/>
            <field name="arch" type="xml">

                <xpath expr="//field[@name='finance_dimension_ids']/tree/field[@name='name']" position="before">
                    <field name="res_id" readonly="1" force_save="1"/>
                </xpath>

            </field>
        </record>


        <!-- Action bs.finance.dimension -->
        <record id="action_bs_finance_dimension_4" model="ir.actions.act_window">
            <field name="name">Finance Dimension 4</field>
            <field name="res_model">bs.finance.dimension</field>
            <field name="view_mode">tree,form</field>
            <field name="domain" eval="[('group_id', '=', ref('bs_finance_dimension_spe.bs_dimension_4'))]"/>
            <field name="context" eval="{'default_group_id':ref('bs_finance_dimension_spe.bs_dimension_4')}"/>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    There is no examples click here to add new Bs Finance Dimension.
                </p>
            </field>
        </record>

