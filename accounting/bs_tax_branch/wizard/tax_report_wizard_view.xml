<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="tax_report_wizard_form_inherited" model="ir.ui.view">
        <field name="name">tax.report.wizard.view.inherited</field>
        <field name="model">tax.report.wizard</field>
        <field name="inherit_id" ref="l10n_th_tax_report.tax_report_wizard_form" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='date_range_id']" position="after">
                <field name="branch_id" options="{'no_create': True}" />
            </xpath>
        </field>
    </record>

</odoo>
